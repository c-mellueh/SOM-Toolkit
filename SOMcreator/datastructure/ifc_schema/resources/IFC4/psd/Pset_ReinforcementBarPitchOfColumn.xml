        <TypePropertySingleValue>
          <DataType type="IfcInteger" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">Ｘ方向巾止め筋本数</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">Ｘ方向巾止筋の本数。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="14e1c980d20011e1800000215ad4efdf">
      <Name>YDirectionTieHoopBarPitch</Name>
