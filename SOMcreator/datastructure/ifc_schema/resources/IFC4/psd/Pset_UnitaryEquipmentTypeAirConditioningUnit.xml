      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcThermodynamicTemperatureMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">凝縮器出口温度</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">凝縮器出口流体温度</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="a149b780d21311e1800000215ad4efdf">
      <Name>OutsideAirFlowrate</Name>
      <Definition>Flow rate of outside air entering the unit.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcVolumetricFlowRateMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">外気流量</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">ユニットに入る外気流量</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">エアコンユニット</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>