<?xml version="1.0"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <Name>Pset_DistributionPortPHistoryPipe</Name>
  <Definition>Log of substance usage attached to an instance of IfcPerformanceHistory having an assigned IfcDistributionPort of type PIPE.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcDistributionPort/GAS</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcDistributionPort/GAS</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="36d77e7919364513b229f61c7632a638">
      <Name>Temperature</Name>
      <Definition>The temperature of the fuel.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcThermodynamicTemperatureMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">温度</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">燃料の温度。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="d31964c0ab4f444aa3a526a72084e705">
      <Name>Pressure</Name>
      <Definition>The pressure of the fuel.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcPressureMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">圧力</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">燃料の圧力。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="7a3f6bee58104037a997d1c2cbd68a4d">
      <Name>Flowrate</Name>
      <Definition>The flowrate of the fuel.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcMassFlowRateMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">流量</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">燃料の流速。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">IfcDistributionPortオブジェクトがGAS型の際に関連するIfcPerformanceHistoryに設定される燃料使用のログ。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>