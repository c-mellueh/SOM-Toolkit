<?xml version="1.0"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <Name>Pset_AirToAirHeatRecoveryPHistory</Name>
  <Definition>Air to Air Heat Recovery performance history common attributes.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcAirToAirHeatRecovery</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcAirToAirHeatRecovery</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="60748300d1c011e1800000215ad4efdf">
      <Name>SensibleEffectiveness</Name>
      <Definition>Sensible heat transfer effectiveness, where effectiveness is defined as the ratio of heat transfer to maximum possible heat transfer.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcNormalisedRatioMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">顕熱効率</NameAlias>
        <NameAlias lang="fr-FR">EfficacitéSensible</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">顕熱効率
効率は、最大可能熱交換に対する熱交換の比率で定義される</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Efficacité d'échange de chaleur sensible, où l'efficacité est définie par le rapport entre l'échange de chaleur effectif et l'échange maximum possible.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="64080a00d1c011e1800000215ad4efdf">
      <Name>TotalEffectiveness</Name>
      <Definition>Total heat transfer effectiveness: The ratio of heat transfer to the maximum possible heat transfer.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcNormalisedRatioMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">全熱効率</NameAlias>
        <NameAlias lang="fr-FR">Rendement thermique</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">全熱効率
最大可能熱交換に対する熱交換の比率</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Rendement thermique: rapport entre la chaleur effective échangée et l'échange maximum possible.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="679b9100d1c011e1800000215ad4efdf">
      <Name>TemperatureEffectiveness</Name>
      <Definition>Temperature heat transfer effectiveness: The ratio of primary airflow temperature changes to maximum possible temperature changes.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcNormalisedRatioMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">温度効率</NameAlias>
        <NameAlias lang="fr-FR">EfficacitéTempérature</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">温度熱交換効率：
最大可能温度変化に対する一次側温度変化の比</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Efficacité thermique sensible: rapport entre la différence de températures pour le flux primaire sur la différence maximale d'échange possible.</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="6c604500d1c011e1800000215ad4efdf">
      <Name>DefrostTemperatureEffectiveness</Name>
      <Definition>Temperature heat transfer effectiveness when defrosting is active.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcNormalisedRatioMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">デフロスト温度効率</NameAlias>
        <NameAlias lang="fr-FR">EfficacitéTemperatureDégel</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">デフロスト作動時の温度熱交換効率</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Efficacité thermique sensible lorsque le mode dégel est actif</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="6f5b3580d1c011e1800000215ad4efdf">
      <Name>HumidityEffectiveness</Name>
      <Definition>Humidity heat transfer effectiveness: The ratio of primary airflow absolute humidity changes to maximum possible absolute humidity changes.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcNormalisedRatioMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">湿度効率</NameAlias>
        <NameAlias lang="fr-FR">EfficacitéLatente</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">湿度熱交換効率：
最大可能絶対湿度変化に対する一次側絶対湿度変化の比</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Efficacité sur transfert de chaleur latente: rapport entre difference de températures</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="73875300d1c011e1800000215ad4efdf">
      <Name>SensibleHeatTransferRate</Name>
      <Definition>Sensible heat transfer rate.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcPowerMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">顕熱交換量</NameAlias>
        <NameAlias lang="fr-FR">PuissanceThermiqueSensible</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">顕熱交換量</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Puissance thermique sensible</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="77b37080d1c011e1800000215ad4efdf">
      <Name>LatentHeatTransferRate</Name>
      <Definition>Latent heat transfer rate.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcPowerMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">潜熱交換量</NameAlias>
        <NameAlias lang="fr-FR">PuissanceThermiqueLatente</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">潜熱交換量</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Puissance thermique latente</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="7b46f780d1c011e1800000215ad4efdf">
      <Name>TotalHeatTransferRate</Name>
      <Definition>Total heat transfer rate.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcPowerMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">全熱交換量</NameAlias>
        <NameAlias lang="fr-FR">PuissanceThermiqueTotale</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">全熱交換量</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Puissance thermique totale</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="7eda7e80d1c011e1800000215ad4efdf">
      <Name>SensibleEffectivenessTable</Name>
      <Definition>Sensible heat transfer effectiveness curve as a function of the primary and secondary air flow rate.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">顕熱効率テーブル</NameAlias>
        <NameAlias lang="fr-FR">DiagrammeEfficacitéSensible</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">一次と二次空気量の関数としての顕熱交換効率曲線</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Courbe d'efficacité d'échange thermique sensible, en tant que fonction du débit d'air au primaire et débit d'air au secondaire</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="826e0580d1c011e1800000215ad4efdf">
      <Name>TotalEffectivenessTable</Name>
      <Definition>Total heat transfer effectiveness curve as a function of the primary and secondary air flow rate.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">全熱効率テーブル</NameAlias>
        <NameAlias lang="fr-FR">DiagrammeEfficacitéTotale</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">一次と二次空気量の関数としての全熱交換効率曲線</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Courbe d'efficacité d'échange thermique total en tant que fonction du débit d'air au primaire et débit d'air au secondaire</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="86018c80d1c011e1800000215ad4efdf">
      <Name>AirPressureDropCurves</Name>
      <Definition>Air pressure drop as function of air flow rate.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">空気圧力降下曲線</NameAlias>
        <NameAlias lang="fr-FR">CourbesPerteChargeAir</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">風量の関数としての空気圧力降下</DefinitionAlias>
        <DefinitionAlias lang="fr-FR">Perte de charge aéraulique fonction du débit d'air</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">空気熱回収装置性能履歴共通属性。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>