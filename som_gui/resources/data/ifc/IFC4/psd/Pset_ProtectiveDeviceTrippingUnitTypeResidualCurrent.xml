  <Name>Pset_ProtectiveDeviceTrippingUnitTypeResidualCurrent</Name>
  <Definition>Information on tripping units that are activated by residual current.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcProtectiveDeviceTrippingUnit/RESIDUALCURRENT</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcProtectiveDeviceTrippingUnit/RESIDUALCURRENT</ApplicableTypeValue>
  <PropertyDefs>
